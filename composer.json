{
    "name": "wearesweet/laravel-revisonable",
    "description": "Apply the trait HasRevisions to any model to be able to: View a history of changes, revert to previous versions of the model and allow for easy moderation of changes before persisting to the database.",
    "authors": [
        {
            "name": "WeAreSweet",
            "email": "developers@wearesweet.co.uk"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/database": "^6|^7",
        "illuminate/support": "^6|^7",
        "illuminate/contracts": "^6|^7",
        "illuminate/events": "^6|^7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4|^9.0"
    },
    "autoload": {
        "psr-4": {
            "WeAreSweet\\LaravelRevisionable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WeAreSweet\\LaravelRevisionable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "WeAreSweet\\LaravelRevisionable\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
